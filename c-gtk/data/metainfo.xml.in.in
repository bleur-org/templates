<component type="desktop-application">
  <id>org.bleur.cgtk</id>
  <metadata_license>CC0</metadata_license>
  <!-- Insert your license of choice here -->
  <!-- <project_license>MIT</project_license> -->
  <name>C GTK Template</name>
  <summary>Write a C + GTK application</summary>
  <description>
    <p>A boilerplate template for C + GTK. It uses Cmake as a build system and has nix support by default.</p>
  </description>
  <url type="homepage">https://github.com/bleur-org/templates</url>
  <url type="bugtracker">https://github.com/bleur-org/templates/issues</url>
  <!-- Source code repository -->
  <url type="vcs-browser">https://github.com/bleur-org/templates</url>
  <!-- Translation repository -->
  <url type="translate">https://github.com/bleur-org/templates/tree/main/c-gtk/po</url>
  <!-- Open Age Ratings Service:
       https://hughsie.github.io/oars/generate.html
  -->
  <content_rating type="oars-1.1" />
  <releases>
    <release version="0.1.0" date="2019-07-11" />
  </releases>
  <kudos>
     <!--
       GNOME Software kudos:
       https://gitlab.gnome.org/GNOME/gnome-software/-/blob/main/doc/kudos.md
     -->
     <kudo>ModernToolkit</kudo>
     <kudo>HiDpiIcon</kudo>
  </kudos>
  <!-- developer_name tag deprecated with Appstream 1.0 -->
  <developer_name translate="no">Bleur</developer_name>
  <!-- Introduced with Appstream 1.0, but not yet fully supported by corresponding projects -->
  <developer id="gnome.org">
    <name translate="no">Bleur</name>
  </developer>
  <!--
    Branding colors:
    https://docs.flathub.org/docs/for-app-authors/metainfo-guidelines/#brand-color
  -->
  <branding>
    <color type="primary" scheme_preference="light">#ff00ff</color>
    <color type="primary" scheme_preference="dark">#993d3d</color>
  </branding>
  <update_contact>support@oss.uzinfocom.uz</update_contact>
  <translation type="gettext">@gettext-package@</translation>
  <launchable type="desktop-id">org.bleur.cgtk.desktop</launchable>
</component>
